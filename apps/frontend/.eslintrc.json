{
  "env": {
    "jest": true
  },
  "extends": [
    "next/core-web-vitals",
    "../../.eslintrc.json",

    "plugin:import/recommended",
    "plugin:import/typescript",
    "next"
  ],
  "parserOptions": {
    "project": "tsconfig.json",
    "tsconfigRootDir": "."
  },
  "plugins": ["react", "testing-library", "jest-dom", "import"],
  "rules": {
    "@typescript-eslint/explicit-function-return-type": "off"
  }
}
